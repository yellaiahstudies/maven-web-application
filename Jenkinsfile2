pipeline {
    agent any
tools{
     maven 'maven3.8.6' 
  }
  
    stages {
        stage('checke out code'){
        steps{
        git branch: 'master', url: 'https://github.com/yellaiahstudies/maven-web-application.git '
        }
      }
        stage('build'){
        steps{
        sh 'mvn clean package'
        }
        }
        stage('build image'){
        steps{
        sh "docker build -t yellaiah56/mavenapp:${env.BUILD_NUMBER} ."
        }
        }
        }
        }
